{
  "name": "fingerprint-generator",
  "version": "2.0.0",
  "description": "NodeJs package for generating realistic browser fingerprints.",
  "homepage": "https://github.com/apify/fingerprint-generator#readme",
  "engines": {
    "node": ">=15.10.0"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "postbuild": "cp -r ./src/data_files ./dist/ && unzip dist/data_files/fingerprint-generator-network -d dist/data_files/",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf ./dist",
    "compile": "tsc -p tsconfig.build.json && gen-esm-wrapper ./dist/index.js ./dist/index.mjs",
    "copy": "ts-node -T ../../scripts/copy.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apify/fingerprint-generator.git"
  },
  "author": "Apify",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/apify/fingerprint-generator/issues"
  },
  "dependencies": {
    "csv-parse": "^4.15.3",
    "generative-bayesian-network": "file:../generative-bayesian-network",
    "header-generator": "file:../header-generator",
    "ow": "^0.24.1",
    "tslib": "^2.3.1"
  }
}
