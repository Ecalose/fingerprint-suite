{
	"name": "fingerprint-injector",
	"version": "2.0.0",
	"description": "Browser fingerprint injection library for playwright and puppeteer.",
	"engines": {
		"node": ">=15.10.0"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"dependencies": {
		"@apify/log": "^1.0.5",
		"fingerprint-generator": "file:../fingerprint-generator",
		"tslib": "^2.3.1",
		"useragent": "^2.3.0"
	},
	"peerDependencies": {
		"playwright": "^1.22.2",
		"puppeteer": "^14.1.1"
	},
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rimraf ./dist",
		"compile": "tsc -p tsconfig.build.json && gen-esm-wrapper ./dist/index.js ./dist/index.mjs",
		"copy": "ts-node -T ../../scripts/copy.ts"
	},
	"author": {
		"name": "Apify",
		"email": "support@apify.com",
		"url": "https://apify.com"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/apify/fingerprint-injector"
	}
}
